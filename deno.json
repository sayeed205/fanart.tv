{
    "name": "@hitarashi/fanart",
    "version": "0.0.0",
    "exports": {
      ".": "./src/main.ts",
      "./types": "./src/types/index.ts"
    },
    "publish": {
      "include": [
        "LICENSE",
        "README.md",
        "src/**/*.ts"
      ],
      "exclude": [
        "tests"
      ]
    },
    "tasks": {
      "dev": "deno run --watch src/main.ts"
    },
    "imports": {
      "@std/assert": "jsr:@std/assert@1",
      "@std/expect": "jsr:@std/expect@^1.0.16",
      "@std/testing": "jsr:@std/testing@^1.0.14"
    },
    "test": {
      "include": [
        "tests/**/*.ts"
      ]
    }
  }
  